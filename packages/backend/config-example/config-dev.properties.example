# 开发环境配置示例文件
# 复制此文件为 config-dev.properties 并填入实际配置值

# Nacos配置中心配置
nacos.ip=127.0.0.1
nacos.port=8848
nacos.namespace=YOUR_DEV_NAMESPACE
nacos.group=sass-cloud
nacos.discoverygroup=sass-cloud
nacos.username=nacos
nacos.password=YOUR_NACOS_PASSWORD

# Seata分布式事务配置
nacos.seata.namespace=YOUR_SEATA_NAMESPACE
seata.ip=127.0.0.1
seata.port=8091

# 日志存储路径
logging.file.path=/data/projects/logs

# 数据库配置（通过环境变量传入）
DB_USERNAME=root
DB_PASSWORD=YOUR_DB_PASSWORD

# Redis配置（通过环境变量传入）
REDIS_PASSWORD=YOUR_REDIS_PASSWORD

# Nacos认证Token（通过环境变量传入）
NACOS_AUTH_TOKEN_KEY=YOUR_NACOS_AUTH_TOKEN_KEY

# Druid监控配置（通过环境变量传入）
DRUID_USERNAME=admin
DRUID_PASSWORD=YOUR_DRUID_PASSWORD