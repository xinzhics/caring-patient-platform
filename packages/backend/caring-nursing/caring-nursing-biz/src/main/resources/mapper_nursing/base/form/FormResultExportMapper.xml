<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.caring.sass.nursing.dao.form.FormResultExportMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.caring.sass.nursing.entity.form.FormResultExport">
        <id column="id" jdbcType="BIGINT" property="id"/>
        <result column="create_user" jdbcType="BIGINT" property="createUser"/>
        <result column="create_time" jdbcType="TIMESTAMP" property="createTime"/>
        <result column="update_user" jdbcType="BIGINT" property="updateUser"/>
        <result column="update_time" jdbcType="TIMESTAMP" property="updateTime"/>
        <result column="export_file_name" jdbcType="VARCHAR" property="exportFileName"/>
        <result column="export_type" jdbcType="VARCHAR" property="exportType"/>
        <result column="plan_id_array_json" jdbcType="VARCHAR" property="planIdArrayJson"/>
        <result column="patient_im_account" jdbcType="VARCHAR" property="patientImAccount"/>
        <result column="base_info_scope_array_json" jdbcType="VARCHAR" property="baseInfoScopeArrayJson"/>
        <result column="export_progress" jdbcType="INTEGER" property="exportProgress"/>
        <result column="export_id" jdbcType="BIGINT" property="exportId"/>
        <result column="export_file_url" jdbcType="VARCHAR" property="exportFileUrl"/>
        <result column="export_total" jdbcType="INTEGER" property="exportTotal"/>
        <result column="message" jdbcType="VARCHAR" property="message"/>
        <result column="org_id" jdbcType="BIGINT" property="organId"/>
        <result column="page_query_json" jdbcType="VARCHAR" property="pageQueryJson"/>
        <result column="current_user_type" jdbcType="VARCHAR" property="currentUserType"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, create_user,create_time,update_user,update_time,
        export_file_name, export_type, plan_id_array_json, base_info_scope_array_json, export_progress,
        patient_im_account, export_id, export_file_url, export_total, message, org_id, page_query_json, current_user_type
    </sql>

    <select id="findPage" resultMap="BaseResultMap">
        SELECT
        <include refid="Base_Column_List"/>
        FROM t_custom_form_result_export s ${ew.customSqlSegment}
    </select>

</mapper>
